<%- include('partials/header') %>

<div class="container-fluid">
  <h6><%=p_complete_answer%></h6>
  <div class="row" style="color: #fff;">

<div class="card text-center" style="width: 30rem; margin: 0px 30px 30px 30px; background-color: #71C9CE">
  <div class="card-body"  >
    <h4 class="card-title">K-Nearest-Neigbor (KNN)</h4>
    <p class="card-text">KNN Binary class<%=p_knn_bin_cls%></p>
    <p class="card-text">KNN Multi class<%=p_knn_mul_cls%></p>
    <p class="card-text">KNN <%=p_knn_desc%></p>
    <p class="card-text">KNN Binary Class Accuracy = <%=p_knn_bin_acc%></p>
    <p class="card-text">KNN Multi Class Accuracy = <%=p_knn_mul_acc%></p>
  </div>
</div>
<div class="card text-center" style="width: 30rem; margin: 0px 30px 30px 30px; background-color: #71C9CE">
  <div class="card-body"  >
    <h4 class="card-title">Random Forest</h4>
    <p class="card-text"><%=p_rf_bin_cls%></p>
    <p class="card-text"><%=p_rf_mul_cls%></p>
    <p class="card-text"><%=p_rf_desc%></p>
    <p class="card-text">Random Forest Binary Class Accuracy = <%=p_rf_bin_acc%></p>
    <p class="card-text">Random Forest Multi Class Accuracy = <%=p_rf_mul_acc%></p>
  </div>
</div>
<div class="card text-center" style="width: 30rem; margin: 0px 30px 30px 30px; background-color: #71C9CE">
  <div class="card-body"  >
    <h4 class="card-title">Convolutional Neural Network (CNN)</h4>
    <p class="card-text"><%=p_cnn_bin_cls%></p>
    <p class="card-text"><%=p_cnn_mul_cls%></p>
    <p class="card-text"><%=p_cnn_desc%></p>
    <p class="card-text">CNN Binary Class Accuracy = <%=p_cnn_bin_acc%></p>
    <p class="card-text">CNN Multi Class Accuracy = <%=p_cnn_mul_acc%></p>
  </div>
</div>
<div class="card text-center" style="width: 30rem; margin: 0px 30px 30px 30px; background-color: #71C9CE">
  <div class="card-body"  >
    <h4 class="card-title">Long Short Term Memory (LSTM) </h4>
    <p class="card-text"><%=p_lstm_bin_cls%></p>
    <p class="card-text"><%=p_lstm_mul_cls%></p>
    <p class="card-text"><%=p_lstm_desc%></p>
    <p class="card-text">LSTM Binary Class Accuracy = <%=p_lstm_bin_acc%></p>
    <p class="card-text">LSTM Multi Class Accuracy = <%=p_lstm_mul_acc%></p>
  </div>
</div>





<div class="login-box">
  <a class="btn btn-success" style="text-align: center;" href="/secrets" role="button">
    Predict Again!
  </a>
  <a class="btn btn-dark" style="text-align: center;background-color: #dd4b39; #dd4b39" href="/logout" role="button">
    LOGOUT
  </a>
  </div>
  </div>
  <%-include('partials/footer') %>


